<h1>Edit existing store</h1>
<br />
<%= form_for @store do |f| %>
  <div class="form-group row">
    <%= f.label :Name, class: "col-sm-1 col-form-label-lg" %>
    <div class="col-sm-4">
      <%= f.text_field :name, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :Street, class: "col-sm-1 col-form-label-lg" %>
    <div class="col-sm-4">
      <%= f.text_field :street, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :Number, class: "col-sm-1 col-form-label-lg" %>
    <div class="col-sm-4">
      <%= f.number_field :number, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :"Zip Code", class: "col-sm-1 col-form-label-lg" %>
    <div class="col-sm-4">
      <%= f.number_field :zipcode, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :City, class: "col-sm-1 col-form-label-lg" %>
    <div class="col-sm-4">
      <%= f.text_field :city, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :Country, class: "col-sm-1 col-form-label-lg" %>
    <div class="col-sm-4">
      <%= f.text_field :country, class: "form-control" %>
    </div>
  </div>
  <br />
  <div class="form-group row">
    <div class="col-sm-4">
      <%= f.submit nil, class: "btn btn-primary" %>
    </div>
  </div>
<% end %>