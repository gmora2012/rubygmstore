<%= form_for @store do |f| %>
<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">Quick Edit Store</h5>
</div>
<div class="modal-body">
  <div class="form-group row">
    <%= f.label :Name, class: "col-sm-3 col-form-label-lg" %>
    <div class="col-sm-5">
      <%= f.text_field :name, class: "form-control", disabled: true %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :Street, class: "col-sm-3 col-form-label-lg" %>
    <div class="col-sm-5">
      <%= f.text_field :street, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :Number, class: "col-sm-3 col-form-label-lg" %>
    <div class="col-sm-5">
      <%= f.number_field :number, class: "form-control", disabled: true %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :"Zip Code", class: "col-sm-3 col-form-label-lg" %>
    <div class="col-sm-5">
      <%= f.number_field :zipcode, class: "form-control", disabled: true %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :City, class: "col-sm-3 col-form-label-lg" %>
    <div class="col-sm-5">
      <%= f.text_field :city, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <%= f.label :Country, class: "col-sm-3 col-form-label-lg" %>
    <div class="col-sm-5">
      <%= f.text_field :country, class: "form-control" %>
    </div>
  </div>
  <br />
</div>
<div class="modal-footer">
    <%= f.submit nil, class: "btn btn-primary" %>
</div>
<% end %>